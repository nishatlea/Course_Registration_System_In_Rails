<p id="notice"><%= notice %></p>

<h1>Enrolls</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Semester</th>
      <th>Curriculum</th>
      <th>Grade</th>
      <th>Semester curriculums</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrolls.each do |enroll| %>
      <tr>
        <td><%= enroll.student_id %></td>
        <td><%= enroll.semester_id %></td>
        <td><%= enroll.curriculum_id %></td>
        <td><%= enroll.grade %></td>
        <td><%= enroll.semester_curriculums_id %></td>
        <td><%= link_to 'Show', enroll %></td>
        <% if current_user.is_admin == true %>
        <td><%= link_to 'Edit', edit_enroll_path(enroll) %></td>
        <td><%= button_to 'Destroy', enroll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if current_user.is_admin == false %>
<%= link_to 'New Enroll', new_enroll_path %>
<% end %>

<p> result: <%= @res %> </p>
